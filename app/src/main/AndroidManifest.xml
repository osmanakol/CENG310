<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group6.stage5_computer_prototype">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Stage5computerprototype">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login_register.LoginActivity" />
        <activity android:name=".ui.login_register.RegisterActivity" />
        <activity
            android:name=".ui.product.list.ProductListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.product.information.ProductInformationActivity"
            android:parentActivityName=".ui.product.list.ProductListActivity" />
    </application>

</manifest>